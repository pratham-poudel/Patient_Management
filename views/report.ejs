<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lab Report</title>
    <style>
       body {
    font-family: Arial, sans-serif;
}

.container {
    max-width: 800px;
    margin: 0 auto;
    padding: 20px;
}

.section {
    margin-bottom: 20px;
    page-break-inside: avoid; /* Ensure sections do not break across pages */
}

.section h2, .section h3 {
    margin-bottom: 10px;
    border-bottom: 1px solid #000000;
    padding-bottom: 5px;
}

.section table {
    width: 100%;
    border-collapse: collapse;
    margin-top: 10px;
    font-size: 12px; /* Adjust font size */
}

.section th, .section td {
    border: 1px solid #000000;
    padding: 4px; /* Adjust padding */
    text-align: left;
}




.right-align p {
            font-weight: bold;
            margin: 0;
            padding: 5px 0;
        }
.header{
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-weight: bold;
    flex-wrap: wrap;
}
.lff{
    h1, h2 {
    margin: 0; /* Removes all margin */
    padding: 0; /* Removes all padding */
    line-height: 1.2; /* Adjusts the spacing between lines (optional) */
}

h1 {
    font-size: 35px; /* Adjust font size to make it fit better */
}

h2 {
    font-size: 16px;
    text-decoration: underline;
}
}
.rtt p{
    margin: 2px;
    padding: 3px 0;
}



    </style>
</head>
<body>
    <div class="container" >
        <div class="header">
            <div class="lff">
                <h1><%=users.medicalName%> </h1>
             <h2>Department of Pathology </h2>
           <div class="right-align">
            <% if (users.date) { %><p>Date: <%= users.date.toDateString() %></p><% } %>
            <% if (users.address) { %><p>Address: <%= users.address %></p><% } %>
        </div>
            </div>
            <div class="rtt">
                <div class="sidema">
                    <% if (users.patientName) { %><p>Patient Name: <%= users.patientName %></p><% } %>
                    <% if (users.age) { %><p>Age: <%= users.age %> years</p><% } %>
                    <% if (users.sex) { %><p>Sex: <%= users.sex %></p><% } %>
                    <% if (users.doctorName) { %><p>Referred By: Dr. <%= users.doctorName %></p><% } %>
                </div>
            </div>
          
    
        </div>
       
       


        
        <% if (users.hb || users.tlc || users.dlc|| users.poly|| users.lympho || users.mono || users.eosino|| users.esr|| users.pcv|| users.bleedingTime|| users.clottingTime|| users.pt|| users.inr || users.plateletCount|| users.reticCount|| users.rbcCount|| users.mcv|| users.mch|| users.mchc ||users.basophil ) { %>
            
            
            
            <div class="section">
                <h2>Hematology Report</h2>
                <table>
                    <tr>
                        <th>Test</th>
                        <th>Result</th>
                        <th>Normal Range</th>
                    </tr>
                    <% if (users.hb) { %>
                        <tr>
                            <td>HB</td>
                            <td><%= users.hb %> gm/dl</td>
                            <td>Male: 13.5-16<br>Female: 12-15.5</td>
                        </tr>
                    <% } %>
                    <% if (users.plateletCount) { %>
                        <tr>
                            <td>Platelet Count</td>
                            <td><%= users.plateletCount %> /Cumm</td>
                            <td>150,000-450,000 /Cumm</td>
                        </tr>
                    <% } %>
                    <% if (users.tlc) { %>
                        <tr>
                            <td>TLC</td>
                            <td><%= users.tlc %> /Cumm</td>
                            <td>4000 - 11000 /Cumm</td>
                        </tr>
                    <% } %>
                    <% if (users.basophil) { %>
                        <tr>
                            <td>Basophil</td>
                            <td><%= users.basophil %> </td>
                            <td> 0.5–1% of white blood cell count</td>
                        </tr>
                    <% } %>
                    <% if (users.dlc) { %>
                        <tr>
                            <td>DLC</td>
                            <td><%= users.dlc %></td>
                            <td></td>
                        </tr>
                    <% } %>
                    <% if (users.poly) { %>
                        <tr>
                            <td>POLY</td>
                            <td><%= users.poly %>%</td>
                            <td>40-75%</td>
                        </tr>
                    <% } %>
                    <% if (users.lympho) { %>
                        <tr>
                            <td>LYMPHO</td>
                            <td><%= users.lympho %>%</td>
                            <td>20-45%</td> 
                        </tr>
                    <% } %>
                    <% if (users.mono) { %>
                        <tr>
                            <td>MONO</td>
                            <td><%= users.mono %>%</td>
                            <td>2-10%</td>
                        </tr>
                    <% } %>
                    <% if (users.eosino) { %>
                        <tr>
                            <td>EOSINO</td>
                            <td><%= users.eosino %>%</td>
                            <td>1-6%</td>
                        </tr>
                    <% } %>
                    <% if (users.basophil) { %>
                        <tr>
                            <td>Basophil</td>
                            <td><%= users.basophil %> </td>
                            <td> 0.5–1% of white blood cell count</td>
                        </tr>
                    <% } %>
                    <% if (users.esr) { %>
                        <tr>
                            <td>ESR</td>
                            <td><%= users.esr %> mm (1<sup>st</sup> Hr.)</td>
                            <td>Male: 0-9<br>Female: 0-20</td>
                        </tr>
                    <% } %>
                    <% if (users.pcv) { %>
                        <tr>
                            <td>PCV</td>
                            <td><%= users.pcv %>%</td>
                            <td>Male: 41-50<br>Female: 36-48</td>
                        </tr>
                    <% } %>
                    <% if (users.bleedingTime) { %>
                        <tr>
                            <td>Bleeding Time</td>
                            <td><%= users.bleedingTime %> min</td>
                            <td>1-6 min</td>
                        </tr>
                    <% } %>
                    <% if (users.clottingTime) { %>
                        <tr>
                            <td>Clotting Time</td>
                            <td><%= users.clottingTime %> min</td>
                            <td>6-10 min</td>
                        </tr>
                    <% } %>
                    <% if (users.pt) { %>
                        <tr>
                            <td>PT</td>
                            <td><%= users.pt %> Sec</td>
                            <td></td>
                        </tr>
                    <% } %>
                    <% if (users.inr) { %>
                        <tr>
                            <td>INR</td>
                            <td><%= users.inr %></td>
                            <td></td>
                        </tr>
                    <% } %>
                   
                    <% if (users.reticCount) { %>
                        <tr>
                            <td>Retic Count</td>
                            <td><%= users.reticCount %>%</td>
                            <td>0.2-2%</td>
                        </tr>
                    <% } %>
                    <% if (users.rbcCount) { %>
                        <tr>
                            <td>RBC Count</td>
                            <td><%= users.rbcCount %> million/Cumm</td>
                            <td>4.6-6.5 million/Cumm</td>
                        </tr>
                    <% } %>
                    <% if (users.mcv) { %>
                        <tr>
                            <td>MCV</td>
                            <td><%= users.mcv %> FI</td>
                            <td>83.0-101 FI</td>
                        </tr>
                    <% } %>
                    <% if (users.mch) { %>
                        <tr>
                            <td>MCH</td>
                            <td><%= users.mch %> PG</td>
                            <td>27-32 PG</td>
                        </tr>
                    <% } %>
                    <% if (users.mchc) { %>
                        <tr>
                            <td>MCHC</td>
                            <td><%= users.mchc %>%</td>
                            <td>33.0-36.0%</td>
                        </tr>
                    <% } %>
                </table>
            </div>
            
            
            
           
            
            
            
            
            
            
            
            
            
            
            <% } %>
            <% if (users.crp||users.raFactor||users.hPylori||users.hiv||users.hbsAgTest||users.hcv||users.vdrl||users.dengue||users.mp||users.troponin) { %>
                
                
                
                <div class="section">
                    <h2>Serological Examination</h2>
                    <table>
                        <tr>
                            <th>Test</th>
                            <th>Result</th>
                        </tr>
                        <% if (users.crp) { %>
                            <tr>
                                <td>CRP</td>
                                <td><%= users.crp %></td>
                            </tr>
                        <% } %>
                        <% if (users.raFactor) { %>
                            <tr>
                                <td>RA Factor</td>
                                <td><%= users.raFactor %></td>
                            </tr>
                        <% } %>
                        <% if (users.hPylori) { %>
                            <tr>
                                <td>H. Pylori</td>
                                <td><%= users.hPylori %></td>
                            </tr>
                        <% } %>
                        <% if (users.hiv) { %>
                            <tr>
                                <td>HIV I & II</td>
                                <td><%= users.hiv %></td>
                            </tr>
                        <% } %>
                        <% if (users.hbsAgTest) { %>
                            <tr>
                                <td>HBs Ag Test</td>
                                <td><%= users.hbsAgTest %></td>
                            </tr>
                        <% } %>
                        <% if (users.hcv) { %>
                            <tr>
                                <td>HCV</td>
                                <td><%= users.hcv %></td>
                            </tr>
                        <% } %>
                        <% if (users.vdrl) { %>
                            <tr>
                                <td>VDRL</td>
                                <td><%= users.vdrl %></td>
                            </tr>
                        <% } %>
                        <% if (users.dengue) { %>
                            <tr>
                                <td>Dengue</td>
                                <td><%= users.dengue %></td>
                            </tr>
                        <% } %>
                        <% if (users.mp) { %>
                            <tr>
                                <td>MP</td>
                                <td><%= users.mp %></td>
                            </tr>
                        <% } %>
                        <% if (users.troponin) { %>
                            <tr>
                                <td>Troponin</td>
                                <td><%= users.troponin %></td>
                            </tr>
                        <% } %>
                    </table>
                </div>
                
                <% } %>

        
        

       
                <% if (users.sTyphiO||users.sTyphiH||users.sParaTyphiAH||users.sParaTyphiBH) { %>
                    <div class="section">
                        <h2>Widal Test</h2>
                        <table>
                            <tr>
                                <th>Test</th>
                                <th>Result</th>
                                <th>Reference Value</th>
                            </tr>
                            <% if (users.sTyphiO) { %>
                                <tr>
                                    <td>S. Typhi 'O'</td>
                                    <td><%= users.sTyphiO %></td>
                                    <td>>1:80 (TITRE)</td>
                                </tr>
                            <% } %>
                            <% if (users.sTyphiH) { %>
                                <tr>
                                    <td>S. Typhi 'H'</td>
                                    <td><%= users.sTyphiH %></td>
                                    <td>>1:160(TITRE)</td>
                                </tr>
                            <% } %>
                            <% if (users.sParaTyphiAH) { %>
                                <tr>
                                    <td>S. Para Typhi 'AH'</td>
                                    <td><%= users.sParaTyphiAH %></td>
                                    <td>>1:320 (TITRE)</td>
                                </tr>
                            <% } %>
                            <% if (users.sParaTyphiBH) { %>
                                <tr>
                                    <td>S. Para Typhi 'BH'</td>
                                    <td><%= users.sParaTyphiBH %></td>
                                    <td>>1:320 (TITRE)</td>
                                </tr>
                            <% } %>
                            <% if (users.noteofwidaltest) { %>
                                <tr>
                                    <td>Note</td>
                                    <td><%= users.noteofwidaltest %></td>
                                </tr>
                            <% } %>
                        </table>
                    </div>
                    <% } %>
        
                    <% if (users.bloodSugarF||users.bloodSugarPP||users.bloodSugarR) { %> <div class="section">
                        <h2>Bio Chemistry Report</h2>
                        <table>
                            <tr>
                                <th>Test</th>
                                <th>Result</th>
                                <th>Normal Range</th>
                            </tr>
                            <% if (users.bloodSugarF) { %>
                                <tr>
                                    <td>Blood Sugar (F)</td>
                                    <td><%= users.bloodSugarF %> mg/dl</td>
                                    <td>60-110 mg/dl</td>
                                </tr>
                            <% } %>
                            <% if (users.bloodSugarPP) { %>
                                <tr>
                                    <td>Blood Sugar (PP)</td>
                                    <td><%= users.bloodSugarPP %> mg/dl</td>
                                    <td>80-140 mg/dl</td>
                                </tr>
                            <% } %>
                            <% if (users.bloodSugarR) { %>
                                <tr>
                                    <td>Blood Sugar (R)</td>
                                    <td><%= users.bloodSugarR %> mg/dl</td>
                                    <td>60-140 mg/dl</td>
                                </tr>
                            <% } %>
                        </table>
                            </div><% } %>
       
                            <% if (users.fasting||users.firstHour||users.secondHour||users.thirdHour) { %>
                                
                                <div class="section">
                                    <h2>Glucose Tolerance Test</h2>
                                <% if (users.fasting) { %>
                                    <table> 
                                    <tr>
                                        <td>Fasting</td>
                                        <td><%= users.fasting %> mg/dl</td>
                                        <td>< 105 mg/dl</td>
                                    </tr>
                                <% } %>
                                <% if (users.firstHour) { %>
                                    <tr>
                                        <td>Glucose Tolerance Test (1st Hour)</td>
                                        <td><%= users.firstHour %> mg/dl</td>
                                        <td>< 195 mg/dl</td>
                                    </tr>
                                <% } %>
                                <% if (users.secondHour) { %>
                                    <tr>
                                        <td>Glucose Tolerance Test (2nd Hour)</td>
                                        <td><%= users.secondHour %> mg/dl</td>
                                        <td>< 145 mg/dl</td>
                                    </tr>
                                <% } %>
                                <% if (users.thirdHour) { %>
                                    <tr>
                                        <td>Glucose Tolerance Test (3rd Hour)</td>
                                        <td><%= users.thirdHour %> mg/dl</td>
                                        <td></td>
                                    </tr>
                                <% } %> 
                            </table>
                        </div>
                                    <% } %> 

                                    
                               
                                    <% if (users.bloodUrea||users.serumCreatinine||users.serumUricAcid) { %><div class="section">
                                        <h2>Renal Function Test</h2>
                                        <table>
                                            <tr>
                                                <th>Test</th>
                                                <th>Result</th>
                                                <th>Normal Range</th>
                                            </tr>
                                            <% if (users.bloodUrea) { %>
                                                <tr>
                                                    <td>Blood Urea</td>
                                                    <td><%= users.bloodUrea %> mg/dl</td>
                                                    <td>15-45 mg/dl</td>
                                                </tr>
                                            <% } %>
                                            <% if (users.serumCreatinine) { %>
                                                <tr>
                                                    <td>Serum Creatinine</td>
                                                    <td><%= users.serumCreatinine %> mg/dl</td>
                                                    <td>0.4-1.4 mg/dl</td>
                                                </tr>
                                            <% } %>
                                            <% if (users.serumUricAcid) { %>
                                                <tr>
                                                    <td>Serum Uric Acid</td>
                                                    <td><%= users.serumUricAcid %> mg/dl</td>
                                                    <td>2-6.0 mg/dl</td>
                                                </tr>
                                            <% } %>
                                        </table>
                                    </div><% } %>



                                    <% if (users.sCholesterol||users.sTriglycerides||users.vldlCholesterol||users.hdlCholesterol||users.ldlCholesterol) { %>
                                        <div class="section">
                                            <h2>Lipid Profile</h2>
                                            <table>
                                                <tr>
                                                    <th>Test</th>
                                                    <th>Result</th>
                                                    <th>Normal Range</th>
                                                </tr>
                                                <% if (users.sCholesterol) { %>
                                                    <tr>
                                                        <td>S. Cholesterol</td>
                                                        <td><%= users.sCholesterol %> mg/dl</td>
                                                        <td>130-200 mg/dl</td>
                                                    </tr>
                                                <% } %>
                                                <% if (users.sTriglycerides) { %>
                                                    <tr>
                                                        <td>S. Triglycerides</td>
                                                        <td><%= users.sTriglycerides %> mg/dl</td>
                                                        <td>50-150 ml/dl</td>
                                                    </tr>
                                                <% } %>
                                                <% if (users.vldlCholesterol) { %>
                                                    <tr>
                                                        <td>VLDL Cholesterol</td>
                                                        <td><%= users.vldlCholesterol %> mg/dl</td>
                                                        <td>2-40 mg/dl</td>
                                                    </tr>
                                                <% } %>
                                                <% if (users.hdlCholesterol) { %>
                                                    <tr>
                                                        <td>HDL Cholesterol</td>
                                                        <td><%= users.hdlCholesterol %> mg/dl</td>
                                                        <td>30-70 mg/dl</td>
                                                    </tr>
                                                <% } %>
                                                <% if (users.ldlCholesterol) { %>
                                                    <tr>
                                                        <td>LDL Cholesterol</td>
                                                        <td><%= users.ldlCholesterol %> mg/dl</td>
                                                        <td>30-175 mg/dl</td>
                                                    </tr>
                                                <% } %>
                                            </table>
                                        </div>
                                        <% } %>
                                        
                                        <% if (users.sBilirubinTotal||users.sBilirubinDirect||users.sProteinsTotal||users.sAlbumin||users.ggtp||users.sAlkalinePhos||users.sgpt||users.sgot) { %>
                                            
                                            <div class="section">
                                                <h2>Liver Function Test</h2>
                                                <table>
                                                    <tr>
                                                        <th>Test</th>
                                                        <th>Result</th>
                                                        <th>Normal Range</th>
                                                    </tr>
                                                    <% if (users.sBilirubinTotal) { %>
                                                        <tr>
                                                            <td>S. Bilirubin (Total)</td>
                                                            <td><%= users.sBilirubinTotal %> mg/dl</td>
                                                            <td>0.4-1.0 mg/dl</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sBilirubinDirect) { %>
                                                        <tr>
                                                            <td>S. Bilirubin (Direct)</td>
                                                            <td><%= users.sBilirubinDirect %> mg/dl</td>
                                                            <td>0.1-0.4 mg/dl</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sProteinsTotal) { %>
                                                        <tr>
                                                            <td>S. Proteins (Total)</td>
                                                            <td><%= users.sProteinsTotal %> mg/dl</td>
                                                            <td>6.0-8.5 mg/dl</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sAlbumin) { %>
                                                        <tr>
                                                            <td>S. Albumin</td>
                                                            <td><%= users.sAlbumin %> mg/dl</td>
                                                            <td>3.7-5.0 mg/dl</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.ggtp) { %>
                                                        <tr>
                                                            <td>GGTP</td>
                                                            <td><%= users.ggtp %> IU/L</td>
                                                            <td>04-38 IU/L</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sAlkalinePhos) { %>
                                                        <tr>
                                                            <td>S. Alkaline Phosphatase</td>
                                                            <td><%= users.sAlkalinePhos %> IU/L</td>
                                                            <td>50-110 IU/L</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sgpt) { %>
                                                        <tr>
                                                            <td>SGPT</td>
                                                            <td><%= users.sgpt %> IU/L</td>
                                                            <td>5-37 IU/L</td>
                                                        </tr>
                                                    <% } %>
                                                    <% if (users.sgot) { %>
                                                        <tr>
                                                            <td>SGOT</td>
                                                            <td><%= users.sgot %> IU/L</td>
                                                            <td>3-45 IU/L</td>
                                                        </tr>
                                                    <% } %>
                                                </table>
                                            </div> 
                                            
                                            <% } %>
                                            <% if (users.sSodium||users.sPotassium||users.sChloride||users.sPhosphorous||users.serumCalcium||users.hba1c||users.amylase|| users.ckMb) { %>
                
                
                                                <div class="section">
                                                    <h2>Electrolyte Test</h2>
                                                    <table>
                                                        <tr>
                                                            <th>Test</th>
                                                            <th>Result</th>
                                                            <th>Normal Range</th>
                                                        </tr>
                                                        <% if (users.sSodium) { %>
                                                            <tr>
                                                                <td>S. Sodium</td>
                                                                <td><%= users.sSodium %> meq/l</td>
                                                                <td>135-155 meq/l</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.sPotassium) { %>
                                                            <tr>
                                                                <td>S. Potassium</td>
                                                                <td><%= users.sPotassium %> meq/l</td>
                                                                <td>3.5-5.5 meq/l</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.sChloride) { %>
                                                            <tr>
                                                                <td>S. Chloride</td>
                                                                <td><%= users.sChloride %> U/L</td>
                                                                <td>98-110 U/L</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.sPhosphorous) { %>
                                                            <tr>
                                                                <td>S. Phosphorous</td>
                                                                <td><%= users.sPhosphorous %> mg%</td>
                                                                <td>2.5-4.8 mg%</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.serumCalcium) { %>
                                                            <tr>
                                                                <td>Serum Calcium</td>
                                                                <td><%= users.serumCalcium %> mg/dl</td>
                                                                <td>8.2-10.4 mg/dl</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.hba1c) { %>
                                                            <tr>
                                                                <td>HbA1c</td>
                                                                <td><%= users.hba1c %> %</td>
                                                                <td>2-7% %</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.amylase) { %>
                                                            <tr>
                                                                <td>Amylase</td>
                                                                <td><%= users.amylase %> U/L</td>
                                                                <td>30-110 U/L</td>
                                                            </tr>
                                                        <% } %>
                                                        <% if (users.ckMb) { %>
                                                            <tr>
                                                                <td>CK-MB</td>
                                                                <td><%= users.ckMb %>%</td>
                                                                <td>5-25% U/L</td>
                                                            </tr>
                                                        <% } %>
                                                    </table>
                                                </div>
                
                
                                                <% } %>
     
    
  
  
                                                <% if (users.colour || users.specificgravity || users.appearance || users.chemicalexamination || users.reaction || users.albumen || users.sugar || users.ketonebodies || users.blood || users.puscells || users.rbc || users.epcells || users.crystals || users.cast || users.others || users.pregnancytest) { %>
                                                    <div class="section">
                                                        <h3 class="section-title">Urine Analysis</h3>
                                                        <table>
                                                            <tr>
                                                                <th>Investigation</th>
                                                                <th>Result</th>
                                                            </tr>
                                                            <% if (users.colour) { %>
                                                                <tr>
                                                                    <td>Colour</td>
                                                                    <td><%= users.colour %></td>
                                                                </tr>
                                                            <% } %>
                                                            <% if (users.specificgravity) { %>
                                                                <tr>
                                                                    <td>Specific Gravity</td>
                                                                    <td><%= users.specificgravity %></td>
                                                                </tr>
                                                            <% } %>
